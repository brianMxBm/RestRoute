{
  "expo": {
    "name": "RestRoute",
    "slug": "restroute",
    "scheme": "rest-route",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "android": {
      "package": "com.restroute.app",
      "versionCode": 1
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.restroute.app",
      "buildNumber": "0.0.1",
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch",
          "audio",
          "voip",
          "remote-notification",
          "nearby-interaction"
        ]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      ["expo-router"],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsVersion": "11.4.0"
        }
      ]
    ]
  }
}
