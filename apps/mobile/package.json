{
  "private": true,
  "name": "@rest-route",
  "version": "1.0.0",
  "main": "index",
  "scripts": {
    "dev": "expo start",
    "lint": "eslint --ext js,ts,tsx .",
    "build": "expo export --output-dir ./build --platform all",
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eas-build-post-install": "bun run -w build:mobile"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.2.29",
    "@expo/metro-runtime": "^3.2.1",
    "@gorhom/bottom-sheet": "4.6.3",
    "@rnmapbox/maps": "10.1.31",
    "@shopify/flash-list": "1.7.1",
    "@tanstack/react-query": "5.49.2",
    "axios": "1.7.7",
    "class-variance-authority": "0.7.0",
    "eas-cli": "12.5.4",
    "expo": "^51.0.17",
    "expo-constants": "~16.0.2",
    "expo-crypto": "~13.0.2",
    "expo-dev-client": "~4.0.19",
    "expo-linking": "~6.3.1",
    "expo-location": "17.0.1",
    "expo-router": "~3.5.23",
    "expo-secure-store": "13.0.2",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "~0.25.18",
    "expo-web-browser": "~13.0.3",
    "nativewind": "^4.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-dotenv": "3.4.11",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-vector-icons": "10.2.0",
    "react-native-web": "~0.19.11",
    "tailwind-merge": "2.2.2",
    "tailwindcss": "^3.4.14"
  },
  "devDependencies": {
    "@rest-route/eslint-config": "workspace:*",
    "@babel/core": "^7.24.5",
    "@types/react": "~18.2.79",
    "@types/react-dom": "~18.2.25",
    "babel-preset-expo": "~11.0.11",
    "react-native-svg-transformer": "1.3.0"
  },
  "eslintConfig": {
    "extends": "@rest-route/eslint-config",
    "ignorePatterns": [
      "node_modules",
      "build",
      ".expo",
      ".expo-shared"
    ],
    "overrides": [
      {
        "files": [
          "*.js"
        ],
        "env": {
          "node": true
        }
      }
    ]
  }
}
