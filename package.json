{
  "private": true,
  "name": "rest-route",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "dev:mobile": "turbo dev --filter=\"{./apps/mobile}...\"",
    "dev:web": "turbo dev --filter=\"{./apps/web}...\"",
    "lint": "turbo lint",
    "test": "turbo test",
    "build": "turbo build",
    "build:mobile": "turbo build --filter=\"...{./apps/mobile}\"",
    "build:web": "turbo build --filter=\"...{./apps/web}\""
  },
  "devDependencies": {
    "turbo": "^1.10.7",
    "typescript": "^4.9.5"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "packageManager": "bun@1.0.25",
  "dependencies": {
    "@types/react-native-vector-icons": "^6.4.18",
    "ajv": "^8.17.1"
  }
}
